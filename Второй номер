Номер1 = float(input("Введите первое число: "))
Номер2 = float(input("Введите второе число: "))
a = Номер1 + Номер2
b = Номер1 - Номер2
c = Номер1 * Номер2
d = Номер1 / Номер2
f = Номер1 % Номер2
print("Сложение:", a)
print("Вычитание:", b)
print("Произведение:", c)
print("Деление:", d)
print("Остаток от деления:", f)
